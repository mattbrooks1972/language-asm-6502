'scopeName': 'source.asm-6502'
'name': '6502 Assembly'
'fileTypes': [
  '6502'
  'S6502'
  'asm6502'
]
'patterns': [
  {
    'comment': 'comment'
    'match': ';.*'
    'name': 'comment.line.semicolon'
  },
  {
    'comment': 'register'
    'match': '\\b([Aa]|[Xx]|[Yy]|[Pp][Cc]|[Ss][Pp]|[Ss][Rr])\\b'
    'name': 'constant.character'
  },
  {
    'comment': 'literal-hexadecimal'
    'match': '\\B([\#][\$][A-Fa-f0-9]+)\\b'
    'name': 'constant.number'
  }
]
